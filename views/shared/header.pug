
header.jheader
    h1.jheader-title
        a(href="/")
            img.jheader-logo(src="/dist/img/logo.svg", alt="JULIANO")
    button#btn-mobilenav.jheader-mobile.btn-link
        span.fa.fa-navicon.fa-lg(role="presentation")
        span.sr-only
            | Toggle Main Menu

    ul#menu-mobilenav.jheader-mobilenav(aria-hidden="true")
        each page in navigations.main
            li
                a(href=page.path) #{page.label}
        each social in navigations.social
            li
                a(href=social.path, target="_blank") #{social.label}

    ul.jheader-nav
        each page in navigations.main
            li
                a(href=page.path) #{page.label}
    ul.jheader-social
        each social in navigations.social
            li
                a(href=social.path, target="_blank")
                    span(role="presentation", class=social.classname)
                    span.sr-only #{social.label}

//-

    header.navbar.navbar-default
        div.container-fluid
            div.navbar-header
                a.navbar-brand(href="/")
                    img(src="dist/img/logo.svg")
            div.navbar-collapse.collapse
                ul.nav.navbar-nav.navbar-right
                    each page in navigations.main
                        li
                            a(href=page.path) #{page.label}
                ul.nav-social.list-inline
                    each social in navigations.social
                        li
                            a(href=social.path, target="_blank")
                                span(role="presentation", class=social.classname)
                                span.sr-only #{social.label}
